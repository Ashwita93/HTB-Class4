{
    "manifest_version": 2,
    "name": "Read-Easy",
    "version": "0.1",
    
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "css": ["style.css"],
        "js": ["jquery-3.4.1.min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }